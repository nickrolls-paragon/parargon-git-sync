{
  "name": "@useparagon/core",
  "version": "1.0.23",
  "description": "Core libraries for paragraph",
  "repository": "https://github.com/useparagon/paragraph",
  "packageManager": "pnpm@8.10.5",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "files": [
    "./dist/",
    "!./dist/.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./src/*/index.ts",
      "require": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.js",
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "cron-parser": "^4.9.0",
    "stacktrace-parser": "^0.1.8",
    "tslib": "2.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.6.0",
    "@types/analytics-node": "^3.1.14",
    "@types/archiver": "^5.3.2",
    "@types/bluebird": "^3.5.42",
    "@types/cloudflare": "^2.7.13",
    "@types/html-pdf": "^3.0.3",
    "@types/jsforce": "^1.11.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.3.7",
    "@types/mailgun-js": "^0.22.18",
    "@types/mssql": "^9.1.4",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.0",
    "@types/number-to-words": "^1.2.3",
    "@types/oauth": "^0.9.4",
    "@types/pg": "^8.10.9",
    "@types/postlight__mercury-parser": "^2.2.7",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/ramda": "^0.29.9",
    "@types/snowflake-sdk": "^1.6.20",
    "@types/ssh2-sftp-client": "^5.2.2",
    "@types/underscore": "^1.11.15",
    "@types/underscore.string": "^0.0.41",
    "@types/unzipper": "^0.10.9",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "aws-sdk": "^2.630.0",
    "cheerio": "^1.0.0-rc.3",
    "csv": "^5.3.2",
    "date-fns": "^2.14.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "fast-xml-parser": "^3.20.3",
    "firebase": "^9.0.2",
    "form-data": "^3.0.0",
    "mailgun.js": "^8.0.2",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "neo4j-driver": "^5.5.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "promise-parallel-throttle": "^3.3.0",
    "redis": "^4.6.7",
    "rimraf": "~4.4.0",
    "sendpulse-api": "^1.1.4",
    "slack": "^11.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^3.42.1",
    "typescript": "^5.3.2",
    "unzipper": "^0.10.11",
    "url": "^0.11.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig.release.json",
    "build:watch": "tsc -b tsconfig.release.json -w",
    "build:dev": "tsc -b tsconfig.json",
    "build:dev:watch": "tsc -b tsconfig.json -w",
    "clean": "rimraf ./dist ./coverage",
    "format": "prettier --ignore-path ../../.prettierignore --config ../../.prettierrc --write .",
    "format:check": "prettier --ignore-path ../../.prettierignore --config ../../.prettierrc --check .",
    "lint": "eslint --ignore-path ../../.eslintignore --config ../../.eslintrc.json --ext .ts .",
    "lint:fix": "eslint --ignore-path ../../.eslintignore --config ../../.eslintrc.json --ext .ts --fix .",
    "release:publish:canary": "pnpm publish --tag next",
    "release:publish:stable": "pnpm publish --tag latest",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts"
  }
}